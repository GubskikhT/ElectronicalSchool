<Window x:Class="ElectronicSchool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElectronicSchool"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="loginButton" Content="Login" HorizontalAlignment="Left" Margin="214,126,0,0" VerticalAlignment="Top" Width="75" Click="loginButton_Click"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="54" Margin="242,199,0,0" VerticalAlignment="Top" Width="192" IsEnabled="{Binding Access}"/>

    </Grid>
</Window>
